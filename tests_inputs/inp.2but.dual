# Molecule information
MOLECULES 2but              # Molecules to load
PRMTOP ligand.prmtop        # Parameter file
INPCRD ligand        # Coordinate / Restart file
RBFILE ligand.rb ligand.rb           # Rigid bodies definition file
FLEXFILE ligand.cart.flex ligand.td.flex        # Flexible bonds definition file
OUTPUT_DIR temp temp
ROOTS 0 0
ROOT_MOBILITY Weld Weld

NONBONDED_METHOD 0 0
NONBONDED_CUTOFF 1.2 1.2

RUN_TYPE Normal
REX_SWAP_EVERY 1
REX_FILE trex.dat

ROUNDS 30 # change
ROUNDS_TILL_REBLOCK 10 10
WORLDS R0 R1
RANDOM_WORLD_ORDER FALSE FALSE
REPRODUCIBLE FALSE FALSE
SEED 666 666

# New keywords
DISTORT_OPTION 0 0          # change
FLOW_OPTION 0 0          # new
WORK_OPTION 0 0          # new
SAMPLERS EMPTY EMPTY         # new
INTEGRATORS OMMVV VV        # new

SAMPLES_PER_ROUND 1 1
TIMESTEPS 0.001 0.001
MDSTEPS 30 1             # change
TEMPERATURE_FIN 300 300        # change
TEMPERATURE_INI 300 300        # change
THERMOSTAT Andersen Andersen
FFSCALE AMBER AMBER
GBSA 0.0 0.0

FIXMAN_POTENTIAL FALSE TRUE
FIXMAN_TORQUE FALSE TRUE

BOOST_TEMPERATURE 300 300    # change
BOOST_MDSTEPS 1 1

PRINT_FREQ 1 0
WRITEPDBS 1 1
VISUAL FALSE FALSE

GEOMETRY FALSE FALSE
DISTANCE 0 1
DIHEDRAL 0 1 2 3


THREADS 0 0
OPENMM TRUE TRUE
OPENMM_CalcOnlyNonbonded FALSE TRUE